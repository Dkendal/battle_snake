<div>
  <h1>title</h1>
    <table>
      <%= for row <- Snek.Server.draw(@world) do %>
        <tr>
          <%= for col <- row do %>
            <td><span><%= col %></span></td>
          <%= end %>
        </tr>
      <% end %>
    </table>
    <pre>
      <%= inspect @world %>
    </pre>
</div>
